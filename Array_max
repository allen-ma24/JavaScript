<body>
    <script>
        function getArrValue(arr=[]){
            let max = arr[0]
            for(let i=1;i<arr.length;i++){
                if(max < arr[i]){
                    max = arr[i]
                }
            }
            return max

        }
        let max = getArrValue([34, 56, 89, 4, 500])
        console.log(max)
        
    </script>
    
</body>
</html>
